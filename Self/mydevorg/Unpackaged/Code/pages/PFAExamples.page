  <!-- define tags replace the pre-defined areas of the template with whatever custom content you'd like  -->
  <apex:define name="left_header"></apex:define>  
  <apex:define name="left_sidebar">Salesforce Services Platform Academy</apex:define>  
  <apex:define name="content_header">Examples and Demos</apex:define>  
  <apex:define name="content_main">
  
<apex:pageBlock >
<apex:pageBlockSection columns="1">
  <apex:outputLink value="{!$Page.PFAcademyTabOrder}">Tab Order</apex:outputLink>
  <apex:outputLink value="{!$Page.PFAcademyVFLifeCycle}">Visualforce Life Cycle</apex:outputLink>
